{
  "app": {
    "name": "PDF重命名工具",
    "executable": "PDF_Rename_Operation.exe",
    "description": "Temu产品测试报告PDF批量重命名工具"
  },
  "repository": {
    "owner": "chen-huai",
    "repo": "Temu_PDF_Rename_APP",
    "api_base": "https://api.github.com",
    "releases_url": "https://github.com/chen-huai/Temu_PDF_Rename_APP/releases"
  },
  "version": {
    "current": "2.0.0",
    "check_interval_days": 30,
    "auto_check_enabled": true,
    "prerelease_enabled": false,
    "version_format": "semantic"
  },
  "update": {
    "backup_count": 3,
    "backup_directory": "backup",
    "download_timeout": 300,
    "max_retries": 3,
    "auto_restart": true,
    "verify_checksum": true,
    "signature_verification": true
  },
  "network": {
    "request_headers": {
      "Accept": "application/vnd.github.v3+json",
      "User-Agent": "PDF-Rename-Tool-Updater/1.0",
      "Connection": "keep-alive"
    },
    "proxy": {
      "enabled": false,
      "host": "",
      "port": 0,
      "username": "",
      "password": ""
    }
  },
  "logging": {
    "enabled": true,
    "level": "INFO",
    "file_path": "auto_updater.log",
    "max_size_mb": 10,
    "backup_count": 5
  },
  "ui": {
    "show_progress": true,
    "show_notifications": true,
    "minimize_to_tray": false,
    "silent_mode": false
  },
  "compatibility": {
    "min_python_version": "3.7",
    "platforms": ["win32", "win64"],
    "architecture": ["x86", "x64"]
  },
  "security": {
    "verify_ssl": true,
    "allowed_hosts": ["github.com", "api.github.com"],
    "block_suspicious_downloads": true
  },
  "features": {
    "auto_backup": true,
    "rollback_support": true,
    "delta_updates": false,
    "background_download": true
  },
  "testing": {
    "mock_mode": false,
    "test_api_endpoint": "https://api.github.com/repos/octocat/Hello-World/releases/latest",
    "force_update": false
  }
}